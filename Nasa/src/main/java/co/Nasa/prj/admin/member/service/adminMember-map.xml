<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.Nasa.prj.admin.member.service.AdminMemberMapper">


	<select id="buyerList" resultType="co.Nasa.prj.comm.VO.BuyerVO">
 		SELECT * FROM buyer
	</select>
	
	<select id="selectBuyer" resultType="co.Nasa.prj.admin.service.AdminAuthorVO">
		SELECT *
		FROM BUYER B JOIN PAYMENT P
		ON(B.B_EMAIL=P.B_EMAIL)
		WHERE B.B_EMAIL= #{b_email}
		
	</select>
	
	<select id="sellerList" resultType="co.Nasa.prj.comm.VO.SellerVO">
		SELECT * FROM SELLER
	</select>
</mapper>